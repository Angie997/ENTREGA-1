// filepath: .vscode/launch.json
{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "firefox",
        "request": "launch",
        "reAttach": true,
        "name": "Launch localhost",
        "url": "http://localhost/index.html",
        "webRoot": "${workspaceFolder}"
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Carteras App",
        "file": "${workspaceFolder}/carteras.html"
      }
    ]
  }